package thiskeyword;

public class Person {
	// private으로 사람 정보를 만들어주세요.
	// 이름, 나이, 돈, 전공
	private String name;
	private int age;
	private int money;
	private String major;
	
	// 생성자를 만들어주세요. 4개 항목을 모두 입력받아 대입해줍니다.
	public Person (String n, int a, int m, String ma) {
		name = n;
		age = a;
		money = m;
		major = ma;
		System.out.println("이름 : " + name + ", 나이 : " + age + ", 돈 : " + money + ", 전공 : " + major);
	}

}
